// Traditional vs Machine Learning-Enhanced Models
digraph {
	A [label="Traditional Models (e.g., Markowitz)" color=lightblue shape=box style=filled]
	B [label="Historical Data" shape=box]
	C [label="Mean-Variance Optimization" shape=box]
	D [label="Optimized Portfolio" color=lightgreen shape=box style=filled]
	A -> B [label=Uses]
	B -> C [label=Applies]
	C -> D [label=Generates]
	E [label="Limitations of Traditional Models" color=lightcoral shape=ellipse style=filled]
	F [label="Assumes Stationarity" shape=box]
	G [label="Ignores Real-Time Data" shape=box]
	H [label="Limited to Historical Data" shape=box]
	A -> E [label=Has]
	E -> F [label="Issue 1"]
	E -> G [label="Issue 2"]
	E -> H [label="Issue 3"]
	I [label="Machine Learning-Enhanced Models" color=lightblue shape=box style=filled]
	J [label="Historical Data + Real-Time Data" shape=box]
	K [label="Sentiment Analysis" shape=box]
	L [label="Stock Price Prediction" shape=box]
	M [label="Enhanced Portfolio Optimization" shape=box]
	N [label="Optimized Portfolio with Real-Time Insights" color=lightgreen shape=box style=filled]
	I -> J [label=Uses]
	J -> K [label=Includes]
	K -> L [label=Feeds]
	L -> M [label=Enhances]
	M -> N [label=Generates]
	O [label="Addressing Limitations with Machine Learning" color=lightcoral shape=ellipse style=filled]
	P [label="Incorporates Real-Time Sentiment Data" shape=box]
	Q [label="Adapts to Market Changes" shape=box]
	R [label="Improves Prediction Accuracy" shape=box]
	I -> O [label=Addresses]
	O -> P [label="Solution 1"]
	O -> Q [label="Solution 2"]
	O -> R [label="Solution 3"]
}
